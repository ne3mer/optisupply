{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    },
    {
      "src": "ethicsupply/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "ethicsupply/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "ethicsupply.settings",
    "SECRET_KEY": "${SECRET_KEY}"
  }
}
